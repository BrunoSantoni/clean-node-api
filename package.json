{
  "name": "clean-node-api",
  "version": "2.5.4",
  "main": "index.js",
  "author": "BrunoSantoni <brunosantoni98@gmail.com>",
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint '. --fix",
      "yarn test:staged"
    ]
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "postbuild": "mkdir dist\\static && cp -r public/** dist/static",
    "prepare": "yarn husky install",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "docker:start": "yarn build && docker-compose up -d",
    "docker:stop": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:clearCache": "jest --clearCache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test --watch -c jest.unit.config.ts",
    "test:integration": "yarn test --watch -c jest.integration.config.ts",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:coveralls": "yarn test:ci && yarn coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.7.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "bson-objectid": "^2.0.3",
    "coveralls": "^3.1.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^12.3.4",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4"
  },
  "dependencies": {
    "apollo-server-express": "^3.6.6",
    "bcrypt": "^5.0.1",
    "express": "^4.17.2",
    "graphql": "^16.3.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^4.3.0",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "16.x"
  }
}
